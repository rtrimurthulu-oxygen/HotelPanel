<div class="row">
    <div class="col-md-12">
        <div class="bgc-white bd bdrs-3 p-20 mB-20">
            <h4 class="c-grey-900 mB-20">Airport List</h4>
            <table id="dataTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>Airport ID</th>
                        <th>Location</th>
                        <th>Name</th>
                        <th>IANATimeZone</th>
                        <th>Code</th>
                        <th>Options</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Airport ID</th>
                        <th>Location</th>
                        <th>Name</th>
                        <th>IANATimeZone</th>
                        <th>Code</th>
                        <th>Options</th>
                    </tr>
                </tfoot>
                <tbody>
                    @foreach (var Item in Model)
                    {
                        <tr>
                            <td>@Item.AirportID</td>
                            <td>@Item.LocationID</td>
                            <td>@Item.Name</td>
                            <td>@Item.IANATimeZone</td>
                            <td>@Item.Code</td>
                            <td>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>